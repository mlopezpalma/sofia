# ============================================
# COPIA ESTE ARCHIVO COMO .env Y CONFIGURA
# ============================================

# REQUERIDO: Tu API key de OpenRouter
# Obtener en: https://openrouter.ai/
OPENROUTER_API_KEY=tu-api-key-aqui

# OPCIONAL: Configuración del servidor
FLASK_PORT=5000
FLASK_DEBUG=False
SECRET_KEY=genera-una-clave-secreta-segura

# OPCIONAL: Límites personalizados
MAX_TOKENS=2000
TEMPERATURE=0.7
MAX_CONTEXT=5